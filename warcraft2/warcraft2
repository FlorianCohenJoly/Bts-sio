1)
    faction(idFaction,nomFaction)
    cle  primaire: idFaction
    
    races(races_id,races_names,races_side)
    
2)create table faction (
idfaction INT,
nomfaction VARCHAR(100),
primary key (idfaction))

3)insert into faction values(1,"alliance")
insert into faction values(2,"horde")

4) alter table races
drop column races_side

alter table races
add idfaction int,

update races set idfaction=1 where races_id=1
update races set idfaction=1 where races_id=3
update races set idfaction=1 where races_id=4
update races set idfaction=1 where races_id=7
update races set idfaction=2 where races_id=2
update races set idfaction=2 where races_id=5
update races set idfaction=2 where races_id=6
update races set idfaction=2 where races_id=8
update races set idfaction=2 where races_id=9
update races set idfaction=2 where races_id=10

5)
 ALTER TABLE races ADD FOREIGN KEY (idfaction) REFERENCES faction (idfaction);
6)
delete from faction where idfaction=1
la contrainte est verifier

7)insert into faction values(3,"neutral")

insert into races values 
(11,"Draeneï","1"), 
(22,"Worgen","1"),
